                            Category
                        </a>
                        <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                            {{#each this.category}}
                            <a class="dropdown-item" href="/course/category/{{this.name}}">{{this.name}}</a>
                            {{/each}}
                        </div>
                    </div>
                    <li class="nav-item">
                        <a class="nav-link home active" href="/">Home<span class="sr-only">(current)</span></a>
